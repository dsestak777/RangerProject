<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rangerproject.WelcomeController">
   <children>
      <Label alignment="CENTER" layoutX="91.0" layoutY="73.0" prefHeight="17.0" prefWidth="422.0" text="Welcome to the Team Ranger Forums!" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <GridPane layoutX="167.0" layoutY="153.0" prefHeight="94.0" prefWidth="270.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="132.0" minWidth="10.0" prefWidth="116.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="216.0" minWidth="10.0" prefWidth="158.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="96.0" minHeight="10.0" prefHeight="85.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="80.0" minHeight="10.0" prefHeight="73.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="98.0" text="Username:" textAlignment="CENTER" />
            <TextField fx:id="name" GridPane.columnIndex="1" />
            <Label alignment="CENTER" layoutX="10.0" layoutY="41.0" prefHeight="17.0" prefWidth="98.0" text="Password:" textAlignment="CENTER" GridPane.rowIndex="1" />
            <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <HBox layoutX="234.0" layoutY="261.0" spacing="25.0">
         <children>
            <Button layoutX="230.0" layoutY="279.0" mnemonicParsing="false" onAction="#hitEnter" text="Enter!!!" />
            <Button layoutX="344.0" layoutY="279.0" mnemonicParsing="false" onAction="#cancel" text="Leave!!" />
         </children>
      </HBox>
      <Label alignment="CENTER" layoutX="91.0" layoutY="123.0" prefHeight="17.0" prefWidth="422.0" text="Enter your Username and Password below then hit enter!" textAlignment="CENTER" />
   </children>
</AnchorPane>
